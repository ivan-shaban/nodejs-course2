{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ivan-shaban/nodejs-course2.git"
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "src/server.ts",
  "scripts": {
    "start": "npm run tsoa:gen && npm run start:dev",
    "start:dev": "ts-node-dev --respawn ./src/server.ts",
    "lint": "eslint src/* --fix",
    "check:types": "tsc",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.(j|t)s\" --noStackTrace",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "tsoa:gen": "tsoa routes && tsoa swagger"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "cross-env": "6.0.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "http-errors": "^1.7.3",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.9",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "4.1.4",
    "tsoa": "^2.5.14",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "ts-node": "^5.0.1",
    "ts-node-dev": "^1.0.0-pre.18",
    "@types/bcrypt": "^3.0.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.3",
    "@types/http-errors": "^1.6.3",
    "@types/mongoose": "^5.7.12",
    "@types/node": "^13.9.5",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "chai": "4.2.0",
    "debug": "4.1.1",
    "dirty-chai": "2.0.1",
    "eslint": "6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "nodemon": "2.0.2",
    "supertest": "4.0.2",
    "typescript": "^3.8.3"
  }
}
